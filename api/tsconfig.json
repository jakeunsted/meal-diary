{
  "compilerOptions": {
    "target": "ESNext",
    "allowImportingTsExtensions": true,
    "moduleResolution": "nodenext",
    "module": "NodeNext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["node"],
    "noEmit": true 
  }
}